      = render partial: 'shared/social'
      a#mobile-menu.btn.btn-theme.collapsed data-target=".main-nav" data-toggle="collapse"
        span
          | Menu
        i.fa.fa-tasks
      = render partial: 'shared/menu'
  /.sidebar-bottom-container
  /.bottom-social
