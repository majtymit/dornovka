      li.facebook
        a href="#" target="_blank" title="Facebook"
          i.fa.fa-facebook
      /li.twitter
        a href="#" target="_blank" title="Twitter"
          i.fa.fa-twitter
      /li.google
        a href="#" target="_blank" title="Google"
          i.fa.fa-google-plus
      /li.linkedin
        a href="#" target="_blank" title="Linkedin"
          i.fa.fa-linkedin
      /li.skype
        a href="#" target="_blank" title="Skype"
          i.fa.fa-skype
      /li.rss
        a href="/rss/" target="_blank" title="Rss Feed"
          i.fa.fa-rss
  /.copyright
